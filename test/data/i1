document.getElementById('selected_color_name').innerHTML = '<span id="spanLoadingSpinner" style="float:right; display:none">Loading... <img src="http://g-ecx.images-amazon.com/images/G/01/ui/loadIndicators/loading-small._V192238939_.gif"></img></span><b class=variationDefault> Color: </b><b class=variationLabel> White </b>';
    amznJQ.available("twister-update-swatch", function() {
      if(typeof unreachableVariations != 'undefined') {
        for(var i in unreachableVariations) {
          for(var j=0 ; j<unreachableVariations[i].length ; ++j) {
            var swatchElement = document.getElementById(i + "_" + j);
            if(swatchElement == null) {
              continue;
            }
            if(!unreachableVariations[i][j] && typeof swatchElement != 'undefined') {
              swatchElement.className = 'swatchUnavailable';
            }
          }
        }
      }
    });
    amznJQ.available("twisterCriticalJS", function() {
        tc_twisterManager((6 == 3 || 6 == 4));
    });
</script>

    <script type="text/javascript">
        window.SizingChart = function() {
            var isOpen = false;
            var open = function(link) {
                amznJQ.available("jQuery", function() {
                    var spinnerContent = "<div id='size-chart-spinner'><img src='"
                        + window.AmazonPopoverImages.snake + "'/></div>";
                    var url = jQuery(link).attr("href");
                    if (window.gOpenQuickview) {
                        window.gOpenQuickview.showSizingChart(url);
                    } else {
                        if(isOpen) return;
                        isOpen = true;
                        amznJQ.available("popover", function() {
                            var popover =  jQuery.AmazonPopover.displayPopover({
                                    literalContent: spinnerContent,
                                    location:"centered",
                                    width:800,
                                    zIndex:991,
                                    height:height,
                                    locationOffset:[0, -50],
                                    title:"",
                                    closeText:"",
                                    onHide: function() {
                                        isOpen = false;
                                    }
                                });

                            var iframeHeight = jQuery(window).height() - 180;
                            var height = Math.min(Math.max(iframeHeight,300), 700);

                            var fetchAjax = function() {
                                jQuery.ajax({
                                    url:url+"&height="+height,
                                    success: function(data) {
                                        data = data.replace(/\s*([^\s]*)\s*$/, "$1");
                                        if(data) {
                                            popover.find(".ap_content").html(data);
                                        } else {
                                            ajaxFailure();
                                        }
                                    },
                                    error: function() {
                                        ajaxFailure();
                                    }
                                });
                            };

                            var ajaxFailure = function() {
                                popover.find(".ap_content").html("<div id='size-chart-error'><p class='error-text'></p><p><a href='" + url + "'></a></p></div>");

                                popover.find("#size-chart-error a").click(function() {
                                    popover.find(".ap_content").html(spinnerContent);
                                    fetchAjax();
                                    return false;
                                });
                            };

                            fetchAjax();
                        });
                    }
                });

                return false;
            };

            return { open : open };
        }();
    </script>







