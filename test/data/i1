function openIVOrChangeImage(event) {
   if(isTwisterJSReady &&goTwisterAltImages ) {
      if(goTwisterAltImages.currentThumbID() == jQuery(event.target).parents(".alt_images_thumb").attr("thumbCount")) {
         event.stopPropagation();
         goTwisterProductImage.triggerIV();
      } else {
         goTwisterAltImages.onMouseOver(jQuery(event.target).parents(".alt_images_thumb").attr("thumbCount"));
      }
   }
}
